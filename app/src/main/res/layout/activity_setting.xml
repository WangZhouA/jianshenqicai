<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F3F3"
    android:orientation="vertical"
    tools:context="fitness_equipment.test.com.fitness_equipment.activity.SettingActivity">

  <include layout="@layout/header_bar_small"></include>

 <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="#F3F3F3"
     ></LinearLayout>
 <RelativeLayout
     android:id="@+id/reClearcache"
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="#FFF"

     >

     <TextView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:text="@string/Clearcache"
         android:gravity="center"
         android:textColor="#242424"
         android:layout_marginLeft="20dp"
         />

     <ImageView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:src="@mipmap/praise_into"
         android:layout_alignParentRight="true"
         android:layout_marginRight="20dp"
         />

 </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CDCDCD"
        android:layout_marginLeft="20dp"
        ></View>
 <RelativeLayout
     android:id="@+id/reRepass"
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="#FFF"

     >

     <TextView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:text="@string/rePass"
         android:gravity="center"
         android:textColor="#242424"
         android:layout_marginLeft="20dp"
         />

     <ImageView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:src="@mipmap/praise_into"
         android:layout_alignParentRight="true"
         android:layout_marginRight="20dp"
         />

 </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CDCDCD"
        android:layout_marginLeft="20dp"
        ></View>
 <RelativeLayout
     android:id="@+id/reHelp"
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="#FFF"

     >

     <TextView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:text="@string/ToHelp"
         android:gravity="center"
         android:textColor="#242424"
         android:layout_marginLeft="20dp"
         />

     <ImageView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:src="@mipmap/praise_into"
         android:layout_alignParentRight="true"
         android:layout_marginRight="20dp"
         />

 </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CDCDCD"
        android:layout_marginLeft="20dp"
        ></View>
 <RelativeLayout
     android:id="@+id/reFeedback"
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="#FFF"

     >

     <TextView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:text="@string/feedback"
         android:gravity="center"
         android:textColor="#242424"
         android:layout_marginLeft="20dp"
         />

     <ImageView
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:src="@mipmap/praise_into"
         android:layout_alignParentRight="true"
         android:layout_marginRight="20dp"
         />

 </RelativeLayout>




</LinearLayout>
